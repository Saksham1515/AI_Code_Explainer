digraph G {
  node [shape=box];

  start [label="Start", shape=circle];
  input [label="Input scores and treeDepth", shape=box];
  init_vars [label="scores = [3, 5, 2, 9, 12, 5, 23, 23]", shape=box];
  init_vars2 [label="treeDepth = math.log(len(scores), 2)", shape=box];
